<tool id="ensembl_variants" name="Perform ensemble variant effect prediction" version="0.1">

<command interpreter="bash">
    ./ensemble.sh $input $output
</command>

<inputs>
<param name="input" format="vcf" type="data" label="VCF file"/>
</inputs>

<outputs>
<data name="output" format="text" type="data"/>
</outputs>
</tool>
